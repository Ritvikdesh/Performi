<body>

<div class="demoLoginDiv ">
    <div class="row divTitle">
        <h1 id="demoLoginTitle">Demo_User Login</h1>
    </div>

    <div class="row">
        <div class="col-5 demoUser ">
            <i id="demoIconColor1" class="fas fa-user"></i>
            <p>Admin</p>
        </div>
        <div class="col-6 demoUser">
            <i id="demoIconColor2" class="fas fa-user"></i>
            <p>HR</p>
        </div>
    </div>

    <div class="row">
        <div class="col-5 demoUser ">
            <i id="demoIconColor3" class="fas fa-user"></i>
            <p class="bottomMargin">Manager</p>
        </div>
        <div class="col-6 demoUser">
            <i id="demoIconColor4" class="fas fa-user"></i>
            <p class="bottomMargin">Employee</p>
        </div>
    </div>

    <section>
        <div id="demoSignIn">Have an account? <a href="/">Sign In</a> </div>
    </section>
</div>



<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.slim.min.js"></script>
